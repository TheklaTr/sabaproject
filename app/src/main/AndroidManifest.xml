<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sabateam.saba">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ExerciseInfo"></activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AvatarScreen"
            android:screenOrientation="landscape" />
        <activity
            android:name=".MenuScreen"
            android:screenOrientation="landscape" />
        <activity
            android:name=".FAQScreen"
            android:screenOrientation="landscape" />
        <activity
            android:name=".BeActiveScreen"
            android:screenOrientation="landscape" />
        <activity
            android:name=".ProgramSelectionScreen"
            android:screenOrientation="landscape" />
        <activity
            android:name=".ProgramScreen"
            android:screenOrientation="landscape" />
    </application>

</manifest>